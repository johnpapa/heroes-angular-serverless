{
  "name": "heroes-angular-serverless",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "ng:serve": "ng serve --proxy-config proxy.conf.json",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "node:reset-db": "node -r dotenv/config --inspect-brk=localhost:9229 server/meta/create-database.js",
    "node:build-ts": "tsc -p server",
    "node:watch-ts": "tsc -w -p server",
    "node:tslint": "tslint -c tslint.json -p server",
    "node:debug": "WWW=./dist/heroes-angular-serverless npx nodemon -r dotenv/config --inspect=9229 server/dist/index.js",
    "node:start": "WWW=./dist/heroes-angular-serverless node -r dotenv/config server/dist/index.js",
    "node-ng:debug": "concurrently \"npm run node:debug\" \"npm run ng:serve\"",
    "node-ng:start": "concurrently \"npm run node:start\" \"npm run ng:serve\"",
    "start": "npm run ng:serve"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.0.2",
    "@angular/common": "~8.0.2",
    "@angular/compiler": "~8.0.2",
    "@angular/core": "~8.0.2",
    "@angular/forms": "~8.0.2",
    "@angular/platform-browser": "~8.0.2",
    "@angular/platform-browser-dynamic": "~8.0.2",
    "@angular/router": "~8.0.2",
    "@azure/cosmos": "^2.1.5",
    "@fortawesome/angular-fontawesome": "^0.3.0",
    "@fortawesome/fontawesome-free": "^5.4.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.7",
    "@fortawesome/free-solid-svg-icons": "^5.4.2",
    "@ngrx/data": "^8.0.1",
    "@ngrx/effects": "^8.0.1",
    "@ngrx/entity": "^8.0.1",
    "@ngrx/store": "^8.0.1",
    "@ngrx/store-devtools": "^8.0.1",
    "angular-font-awesome": "^3.1.2",
    "bulma": "^0.7.4",
    "core-js": "^2.5.4",
    "font-awesome": "^4.7.0",
    "rxjs": "~6.5.2",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.0",
    "@angular/cli": "~8.0.3",
    "@angular/compiler-cli": "~8.0.2",
    "@angular/language-service": "~8.0.2",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.1",
    "concurrently": "^4.1.0",
    "dotenv": "^7.0.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.4.5"
  }
}
